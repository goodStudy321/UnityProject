	passive_buff passive_buff	    [option = "属性加成"];
	self_buf self_buff  		    [option = "加BUFF对己"];
	direct_buf direct_buff  		[option = "加BUFF对敌"];
	hit_buf hit_buff     			[option = "加BUFF概率"];
	consume_items level_up_consume	[option = "升级消耗"];

