message SkillLevelAttr [key=level_skill_id, source="J 技能等级配置表.xls", sheet="Sheet1"]
{	
	struct passive_buff [separator = '_']
	{
		uint32 buff_ID;	//buff_ID;
	};
	
	struct direct_buf [separator = '_']
	{
		uint32 buff_ID;	//buff_ID;
	};
	
	struct hit_buf    [separator = '_']
	{
		uint32 buff_ID;	//buff_ID;
	};
	
	struct self_buf    [separator = '_']
	{
