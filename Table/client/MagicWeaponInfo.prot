message MagicWeaponInfo [key=id, source="F 法宝基础表.xls", sheet="Sheet1"]
{
	struct array [separator = ',']
	{
		uint32 value;
	};
	
	uint32 id						    [option = "ID"];
	string name                         [option = "名称"];
	uint16 model_id						[option = "模型"];
	uint16 ui_model_id					[option = "UI模型"];
	uint8 root							[option = "挂载点"];
	string icon_path					[option = "图标"]; 
};