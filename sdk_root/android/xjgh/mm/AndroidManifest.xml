<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.hyyx.tdwq">
    <!-- 震动权限 -->
    <uses-permission android:name="android.permission.VIBRATE" />
    <uses-permission android:name="android.webkit.permission.PLUGIN" />
    <!-- 读取电话和短信状态 -->
    <uses-permission android:name="android.permission.READ_PHONE_STATE" />
    <!-- 网络访问权限 -->
    <uses-permission android:name="android.permission.INTERNET" />
    <!-- 访问网络状态权限 -->
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <!-- 访问WiFi状态信息权限 -->
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />

    <uses-permission android:name="android.permission.MEDIA_MOUNTED" />
    <!-- 结束后台权限 -->
    <uses-permission android:name="android.permission.KILL_BACKGROUND_PROCESSES" />
    <!-- 存储权限 -->
    <uses-permission android:name="android.permission.MOUNT_UNMOUNT_FILESYSTEM"/>
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:name="com.hyyx.tdwq.XJApplication"
        android:icon="@drawable/app_icon"
        android:label="@string/app_name"
        android:theme="@style/UnityThemeSelector"
        android:usesCleartextTraffic="true"
        android:networkSecurityConfig="@xml/network_security_config">

        <activity
            android:name="com.hyyx.tdwq.hygameActivity"
            android:resizeableActivity="true"
            android:windowSoftInputMode="stateHidden|adjustResize"
            android:configChanges="orientation|keyboardHidden|navigation|screenSize">

            <meta-data android:name="unityplayer.UnityActivity" android:value="true"/>
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <receiver
            android:name="com.bytedance.applog.collector.Collector"
            android:enabled="true"
            android:exported="false" />

        <!-- 联系客服窗口 -->
        <activity
            android:name="com.xinji.sdk.activity.CustomServiceActivity"
            android:configChanges="orientation|keyboardHidden|screenSize"/>
        <!-- 高版本下无HttpClient配置 -->
        <uses-library
            android:name="org.apache.http.legacy"
            android:required="false" />

